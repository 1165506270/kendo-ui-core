
        /// <summary>
        /// <%= description %>
        /// </summary>
<% if type == "stringArray" %>        [TypeConverter(typeof(Telerik.Web.Design.ListConverter))]
<% end %>        [DefaultValue(<%= csharp_default %>)]
        public <%= csharp_class %> <%= csharp_name %>
        {
            get
            {
                return (<%= csharp_class %>)(ViewState["<%= csharp_name %>"] ?? <%= csharp_default %>);
            }
            set
            {
                ViewState["<%= csharp_name %>"] = value;
            }
        }
