<!doctype html>
<html>
<head>
    <title>Kendo UI Examples</title>
    <meta charset="utf-8">
    <link rel="stylesheet" href="content/shared/styles/suite.css" />
</head>
<body>
    <div id="page">
        <ul>
            <% categories.each do |category| %>
            <li>
                <h2><%= category['text'] %></h2>
                <ul>
                    <% category['items'].each do |item| %>
                        <li><a href='<%= item['url']%>.html'><%= item['text'] %></a></li>
                    <% end %>
                </ul>
            </li>
        <% end %>
        </ul>
    </div>
</body>
</html>
