
        <div id="example" class="k-content">
            
            <div class="demo-section" style="width: 250px;">
                <label for="titles">Select Title:</label> <input id="titles"/>
            </div>

            <script>
                $(document).ready(function() {
                    $("#titles").kendoDropDownList({
                        dataTextField: "ProductName",
