        <Icon guid="guidImages" id="lintIcon" />
        <Strings>
          <CommandName>Analyze Kendo UI code usage</CommandName>
          <ButtonText>Analyze Kendo UI code usage</ButtonText>
        </Strings>
      </Button>

      <Button type="Button" guid="guidKendoBootstrapperCmdSet" id="IdKendoLintDocumentationContextCommand" priority="0x0100">
        <Parent guid="guidKendoBootstrapperCmdSet" id="IdKendoLintDocumentationContextGroup" />
        <Icon guid="guidImages" id="docsIcon" />
        <CommandFlag>DynamicVisibility</CommandFlag>
        <Strings>
          <CommandName>View Kendo UI Documentation</CommandName>
          <ButtonText>View Kendo UI Documentation</ButtonText>
        </Strings>
      </Button>
    </Buttons>

    <Bitmaps>
